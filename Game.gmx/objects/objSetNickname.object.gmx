          <argument>
            <kind>1</kind>
            <string>/// Draw
if ( shake &gt; 0 ) {
    shakeit = irandom_range(shake, -shake);
    shake -= 0.5;
}
if ( step[0] &lt; 60 )
    step[0]++;
if ( step[1] &lt; 150 ) and ( step[0] == 60 )
    step[1]++;
if ( step[2] &lt; 80 ) and ( step[1] == 150 )
    step[2]++;
if ( step[3] &lt; 60 ) and ( step[2] == 80 )
    step[3]++;
if ( step[3] &gt; 0 ) {
    if ( mouse_check_button_pressed(mb_left) ) {
        if ( mouse_y &lt; room_height - 110 ) and ( goto_next == false ) {
            msg = get_string_async("당신의 이름은?", "");
        }
        if ( mouse_y &gt; room_height - 100 ) {
            click = true;
        }
    }
    if ( mouse_check_button_released(mb_left)) {
        if ( mouse_y &gt; room_height - 100 ) and ( click == true ){
            event_user(0);
        }
        
        click = false;
    }
    if ( mouse_y &lt; room_height - 100 ) {
        click = false;
    }
}
hj_change_font_ext(sprHangul60, sprEng60, 60, 60, 35, 60);
draw_set_color(c_black);
hj_set_align(fa_center, fa_middle)
if(nonickname == "") {
hj_draw(room_width/2, room_height/2 - 40 - ease_in_out_back((1/120)*step[2])*300, "안녕? 친구야", c_black, ease_in_cubic((1/60)*min(60, step[0])));
draw_set_alpha(ease_in_cubic((1/60)*min(60, step[1])));
if(global.asdf == false)
hj_draw(room_width/2, room_height/2 + 40 - ease_in_out_back((1/120)*step[2])*300, "너의 이름은 뭐니?", c_black, ease_in_cubic((1/60)*min(60, step[1])));
else
hj_draw(room_width/2, room_height/2 + 40 - ease_in_out_back((1/120)*step[2])*300, "낯이 익다?", c_black, ease_in_cubic((1/60)*min(60, step[1])));

}else {
hj_set_align(fa_center, fa_top)
hj_draw(room_width/2 + shakeit, room_height/2 + 40 - ease_in_out_back((1/120)*step[2])*300, nonickname, nonickname_color, 1);
hj_set_align(fa_center, fa_middle)
}
draw_set_alpha(ease_in_out_cubic((1/60)*step[3]));

hj_change_font_ext(sprHangul120, sprEng120, 120, 120, 70, 120);
hj_draw(room_width/2, room_height/2 + 45, global.nickname + ttokttak, c_black, ease_in_out_cubic((1/60)*step[3]));
hj_change_font_ext(sprHangul50, sprEng50, 50, 50, 29, 50);
hj_draw(room_width/2, 50, "화면의 아무곳을 눌러#당신의 이름을 입력하세요", c_black, ease_in_out_cubic((1/60)*step[3]));
hj_change_font_ext(sprHangul60, sprEng60, 60, 60, 35, 60);
draw_rectangle(0, room_height - 110, room_width, room_height, false);
draw_set_color(c_white);
if ( click == true )
    scale += ( 0.8 - scale )/10;
else
    scale += ( 1 - scale ) / 10;
hj_draw_transformed(room_width/2, room_height - 60, "다음으로 넘어가기 (R)", scale, scale, 0, c_white, ease_in_out_cubic((1/60)*step[3]));
if ( step[3] &gt; 0 ) {
    hj_draw_transformed(room_width/2, room_height - 60, "다음으로 넘어가기 (R)", scale, scale, 0, c_black, 1 - scale);
}

if ( goto_next ) {
    step[4] += 0.05;
    draw_set_color(c_white);
    draw_set_alpha(-2.5+step[4]);
    draw_rectangle(0, 0, room_width, room_height, false);
    if ( step[4] &gt; 3.5 )
        room_goto_next();
}
